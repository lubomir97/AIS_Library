@model IEnumerable<AIS_Library.Models.Users.User>
@{
    ViewBag.Title = "Список читачів, яких обслужив бібліотекар";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Список читачів, яких обслужив бібліотекар</h2>


<table align="center" class="table-condensed" id="table_id">
    <thead>
        <tr>
            <th>Прізвище</th>
            <th>Ім'я</th>
            <th>По-батькові</th>
            <th>Адреса проживання</th>
            <th>Мобільний телефон</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.Surname</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
            </tr>
        }
    </tbody>
</table>
