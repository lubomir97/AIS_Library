@model IEnumerable<AIS_Library.Models.Users.User>

@{
    ViewBag.Title = "Список користувачів, які не відвідували бібліотеку";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Список користувачів, які не відвідували бібліотеку протягом цього часу</h2>
@using (Html.BeginForm("NoVisit", "ManageUser", FormMethod.Post))
{

    <div class="col-md-6">
        <h5></h5>
        <br />
        <input type="date" id="DateId" name="start" value="" />
        <br /><br /><br /><br /><br /><br />
        <input type="date" id="DateId" name="end" value="" />

        <p align="center"><input type="submit" , value="Знайти" /></p>
    </div>
}

<table align="center" class="table-condensed" id="table_id">
    <thead>
        <tr>
            <th>Прізвище</th>
            <th>Ім'я</th>
            <th>По-батькові</th>
            <th>Адреса проживання</th>
            <th>Мобільний телефон</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.Surname</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>

            </tr>
        }
    </tbody>
</table>



